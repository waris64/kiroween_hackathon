# Test Environment Configuration
# Used during Jest test execution

# Server Configuration
PORT=3001
NODE_ENV=test

# Google Gemini AI Configuration (use test key or mock)
GEMINI_API_KEY=test_api_key_for_testing

# CORS Configuration
CORS_ORIGIN=http://localhost:5173

# Rate Limiting (relaxed for tests)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# Cache Configuration (short TTL for tests)
CACHE_TTL_SECONDS=10

# Git Analysis
MAX_REPO_SIZE_MB=100
TEMP_CLONE_DIR=./temp/test-repos

# Logging
LOG_LEVEL=error
